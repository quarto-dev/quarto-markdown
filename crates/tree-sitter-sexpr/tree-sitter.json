{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "sexpr",
      "camelcase": "Sexpr",
      "title": "Sexpr",
      "scope": "source.sexpr",
      "file-types": [
        "sexpr"
      ],
      "injection-regex": "^sexpr$",
      "class-name": "TreeSitterSexpr"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "sexpr parser",
    "authors": [
      {
        "name": "CES",
        "email": "ces@no.com"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-sexpr"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}